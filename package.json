{
  "name": "vue-deprecate",
  "version": "0.1.0",
  "author": "fringers",
  "bugs": {
    "url": "https://github.com/fringers/vue-deprecate/issues"
  },
  "homepage": "https://github.com/fringers/vue-deprecate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fringers/vue-deprecate.git"
  },
  "keywords": [
    "deprecate",
    "vue",
    "vue.js"
  ],
  "license": "MIT",
  "main": "dist/vue-deprecate.umd.js",
  "scripts": {
    "build": "vue-cli-service build --target lib src/deprecate.js --mode production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  }
}
